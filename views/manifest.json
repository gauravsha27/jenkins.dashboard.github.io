{
  "manifest_version": 2,
  "name": "Jenkins Extension",
  "version": "1.0",
  "description": "This is to view a consolidated report of all jenkins details",
  "icons":{
    "128": "images/jenkins_icon_128.png",
    "48": "images/jenkins_icon_48.png",
    "16": "images/jenkins_icon_16.png"
  },
  "browser_action": {
    "default_icon": "images/jenkins_icon_16.png",
    "default_popup": "views/popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "storage",
    "notifications"
  ]
}
